{
  "expo": {
    "name": "Chatty",
    "slug": "chatty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chatty.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chatty.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.krsahil.chatty"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/chatty.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chatty.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b78e019-8151-4059-beb0-f833578872d3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2b78e019-8151-4059-beb0-f833578872d3"
    }
  }
}
